#include <stdio.h>
int main(void)
{
    int num_data[24];
    for (int i=0;i<24;i++){
        num_data[i] = i+1;
         

    }
    int f;
    int b ;
    printf("Please enter number : ");
    scanf("%d", &b);
    for (int i = 0;i<24;i++){
        if(b == num_data[i]){
            f = 1;

        }
    }
    if(f==1){
        printf("enter number :%d\n",b);
        for( int i=1; i<=12; i++ ) {
        
        printf("%d x %d = %d\n", b, i, (b*i));
    }
    }else{
        printf("Error!! Please enter number(1-24)");
    }
}
